<h1>Backbone Colleges</h1>

<table>
  <thead>
    <tr>
      <th>College Name</th>
      <th>Established year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% for college in @colleges.models: %>
      <tr>
        <td><%= college.get('name') %></td>
        <td><%= college.get('established_year') %></td>
      </tr>
    <% end %>
  </tbody>
</table>
